    {
      "cell_type": "code",
      "metadata": {
        "id": "sen85C2Bp1pJ"
      },
      "source": [
        "summaries = []\n",
        "for summary in predictions.predicted_title:\n",
        "  summary = summary.strip()\n",
        "  summaries.append(summary)\n",
        "\n",
        "references = []\n",
        "for reference in predictions.real_title:\n",
        "  reference = reference.strip()\n",
        "  references.append(reference)"
      ],
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "aMpltK4rp-fC"
      },
